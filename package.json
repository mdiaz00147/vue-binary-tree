{
  "name": "vue-binary-tree",
  "version": "1.2.1",
  "description": "A vue2 component to display binary trees",
  "author": "mario@blackboxcode.io",
  "main": "./src/components/BinaryTree",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdiaz00147/Vue-Binary-Tree.git"
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "vue",
    "tree",
    "binary",
    "binary tree"
  ],
  "license": "MIT",
  "homepage": "https://github.com/mdiaz00147/Vue-Binary-Tree/blob/master/README.md",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --dest dist --name BinaryTree ./src/components/BinaryTree",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "floating-vue": "^1.0.0-beta.18",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-service": "^3.9.2",
    "vue-template-compiler": "^2.6.10",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
